{
    "name": "gusmanson/localtinymce",
    "description": "A simple, self-hosted TinyMCE with image uploads for Laravel Nova.",
    "authors":
    [
        {
            "name": "Martijn Gussekloo",
            "email": "mgussekloo@gusmanson.nl"
        }
    ],
    "keywords":
    [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require":
    {
        "php": "^8.0",
        "laravel/nova": "^4.0",
        "tinymce/tinymce": ">= 5"
    },
    "autoload":
    {
        "psr-4":
        {
            "Gusmanson\\Localtinymce\\": "src/"
        }
    },
    "repositories":
    [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra":
    {
        "laravel":
        {
            "providers":
            [
                "Gusmanson\\Localtinymce\\FieldServiceProvider"
            ]
        }
    },
    "config":
    {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}