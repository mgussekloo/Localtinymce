{
    "name": "localtinymce",
    "description": "A simple, self-hosted TinyMCE with image uploads for Laravel Nova.",
    "authors":
    [
        {
            "name": "Martijn Gussekloo",
            "email": "mgussekloo@gusmanson.nl"
        }
    ],
    "private": true,
    "scripts":
    {
        "dev": "mix",
        "watch": "mix watch -- --watch-options-poll=1000",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "production": "mix --production",
        "nova:install": "npm --prefix='../../vendor/laravel/nova' ci"
    },
    "devDependencies":
    {
        "cross-env": "^7.0.3",
        "laravel-mix": "^6.0",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.75.0",
        "sass-loader": "^12.1.0",
        "vue-loader": "^16.2.0"
    },
    "dependencies":
    {
        "@tinymce/tinymce-vue": "^5.1.1",
        "vue": "^3.4.23"
    }
}